{
  "name": "auth0-lock-passwordless",
  "version": "0.0.0",
  "main": "src/index.js",
  "license": "MIT",
  "browserify": {
    "transform": [
      [
        "browserify-css",
        {
          "minify": true
        }
      ],
      [
        "babelify",
        {
          "blacklist": [
            "regenerator"
          ],
          "optional": [
            "es7.functionBind",
            "es7.objectRestSpread"
          ]
        }
      ],
      [
        "packageify"
      ]
    ]
  },
  "scripts": {
    "start": "grunt dev",
    "build": "grunt build",
    "test": "mochify --recursive --extension=.jsx",
    "test:browser": "zuul --local 8080 -- test/*/*.test.js",
    "test:saucelabs": "zuul -- test/*/*.test.js"
  },
  "devDependencies": {
    "babelify": "^6.1.2",
    "browserify": "^10.2.4",
    "browserify-css": "^0.6.1",
    "bump": "git://github.com/ianstormtaylor/bump",
    "expect.js": "^0.3.1",
    "grunt": "^0.4.5",
    "grunt-aws-s3": "^0.14.0",
    "grunt-browserify": "^4.0.0",
    "grunt-cli": "^0.1.13",
    "grunt-contrib-clean": "^0.6.0",
    "grunt-contrib-connect": "^0.11.2",
    "grunt-contrib-copy": "^0.8.0",
    "grunt-contrib-uglify": "^0.9.1",
    "grunt-env": "^0.4.4",
    "grunt-http": "^1.6.0",
    "mocha": "^2.2.5",
    "mochify": "^2.12.0",
    "packageify": "^0.2.2",
    "request": "^2.58.0",
    "sinon": "^1.15.4",
    "uglify-js": "^2.4.23",
    "unreleased": "^0.1.0",
    "watchify": "^3.2.2",
    "zuul": "^3.2.0"
  },
  "dependencies": {
    "auth0-js": "^6.5.0",
    "blueimp-md5": "^1.1.0",
    "bonzo": "^2.0.0",
    "domready": "^1.0.8",
    "flux": "^2.0.3",
    "immutable": "^3.7.3",
    "jsonp": "^0.2.0",
    "react": "^0.13.3",
    "serve": "*",
    "trim": "0.0.1",
    "underscore": "^1.8.3"
  }
